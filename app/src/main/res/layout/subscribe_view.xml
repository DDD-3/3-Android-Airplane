<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>


    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <!-- 구독자수 -->
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_subscribe"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_sample" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_subscribe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@id/iv_subscribe"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="20" />

        <!-- 참여자수 -->
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_live"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@id/tv_subscribe"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_sample" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_live"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@id/iv_live"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="31" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>